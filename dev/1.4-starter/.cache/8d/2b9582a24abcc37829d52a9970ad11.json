w-reverse;\n    flex-wrap: wrap;\n    padding: 0 5vw;\n    margin: 0 auto;\n    max-width: 1200px;\n\n    font-weight: 400;\n    font-style: normal;\n}\n\nmain {\n    width: 80vw;\n    max-width: 700px;\n}\n\ndiv > header {\n    display: flex;\n    justify-content: space-between;\n    align-items: center;\n}\n\ndiv > header a {\n    color: var(--base);\n}\n\nmain ul,\nmain ul li {\n    margin: 0;\n    padding: 0;\n    list-style: none;\n}\n\nmain > ul {\n    padding: 2ch 0;\n    column-count: 3;\n    column-gap: 1rem;\n}\n\nmain > ul > li {\n    break-inside: avoid;\n    margin-bottom: 1rem;\n}\n\nmain > ul > li:last-child {\n    clear: both;\n}\n\nmain li p {\n    background-color: var(--secondary-highlight);\n    color: var(--primary-shade);\n    display: block;\n    margin: 0 0 3ch 0;\n    padding: 2ch;\n}\n\nmain li p a {\n    text-decoration: none;\n    color: var(--primary-shade);\n    font-weight: 900;\n}\n\nfigure {\n    width: 100%;\n    margin: 0;\n}\n\nimg {\n    width: 100%;\n    height: auto;\n}\n\nheader,\nfooter {\n    width: 100%;\n}\n\nbody > div > aside {\n    flex: 1;\n    padding-right: 6ch;\n}\n\naside > ul > li:not(:last-child) {\n    margin: 0 0 2ch 0;\n    padding: 0 0 2ch 0;\n    list-style: none;\n    border-bottom: 1px solid var(--secondary-shade);\n}\n\naside > ul {\n    margin: 0;\n    padding: 0;\n    list-style: none;\n}\n\nmain header {\n    background-color: var(--secondary-highlight);\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    padding: 2% 5%;\n    border-top-left-radius: 10px;\n    border-top-right-radius: 10px;\n    color: var(--primary-shade);\n}\n\n/*\n * Typography\n */\n\nh1 {\n    font-family: circe,sans-serif;\n    font-weight: 800;\n    font-style: normal;\n    font-size: 1.25rem;\n\n    border-radius: 999px;\n\n    /* background-color: var(--secondary-highlight); */\n    color: var(--white);\n\n    display: inline-block;\n    margin: 3ch 0;\n}\n\nh2 {\n    font-family: circe, sans-serif;\n    font-weight: 700;\n    font-style: normal;\n    color: var(--primary-shade);\n}\n\n/*\n * Interactions\n */\n\n button {\n     cursor: pointer;\n     background-color: var(--secondary-highlight);\n     font-size: 1.2rem;\n     border: none;\n     border-radius: 10px;\n     padding: 1ch 2ch;\n     border-bottom-width: 5px;\n     border-bottom-style: solid;\n     border-bottom-color: transparent;\n     border-top-width: 5px;\n     border-top-style: solid;\n     border-top-color: transparent;\n     -webkit-appearance: none;\n        -moz-appearance: none;\n             appearance: none;\n }\n\n button[aria-selected=\"true\"] {\n    background-color: var(--secondary-highlight);\n }\n\n button:not([aria-selected=\"true\"]):hover {\n    border-bottom-color: var(--primary-shade);\n};"},"lineCount":null}},"error":null,"hash":"0c7bdafedf0d97a6c02deb5fb56f64e8","cacheData":{"env":{}}}