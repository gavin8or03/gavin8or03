 window.addEventListener(\"DOMContentLoaded\", () => {\n        \n        bodyMain.appendChild(newElementContainer);\n        fetch(url)\n            .then(res => res.json())\n            .then(data => handleAPI(data.data))\n            .catch(err => console.error(err.message, err.name));\n    \n    \n        });\n\n    const handleAPI = (API_DATA) => {\n        const gifElement = ({images, url, title, bitly_url}) => (\n            `\n                <img src=\"${images.original.url}\" alt=\"gif of ${title}\" loading=\"lazy\" />\n                <p>\n                    <a href=\"${url}\" title=\"See original on Giphy\" target=\"_blank\" rel=\"noopener noreferrer\">${title}</a>\n                </p>\n             `\n        )\n\n        API_DATA.forEach(el => {\n            const newElement = document.createElement(`li`);\n            newElement.setAttribute('data-gif-id', el.id);\n            newElement.innerHTML = gifElement(el);  \n            newElementContainer.appendChild(newElement);\n        })\n    }\n}\n\nexport default API;"},"lineCount":null}},"error":null,"hash":"d1f81a1d86e51e2455eac1ae726b2a34","cacheData":{"env":{}}}