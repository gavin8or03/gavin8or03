lessons/lesson-three.js":"const promises = () => {\n    const url = `http://lookup-service-prod.mlb.com/json/named.roster_40.bam?team_id='147'`\n    // Where we'll make our request to\n    // Change the number value on the end to find other MLB teams!\n\n    const fetchTeam = fetch(url, {\n        method: 'GET'\n    })\n    // Our API requires us to set the GET method\n\n    console.log(\"fetchTeam: \", fetchTeam)   // still a promise!\n\n    const theTeam = fetchTeam               // initial fetch is a promise, must be resolved\n        .then(response => response.json())  // response is an object, convert to json\n        .then(team => handleResponse(team)) // response is now json and can be handled\n        .catch(error => console.log(\"Error: \", error))\n\n    const handleResponse = (res) => {\n        buildteam(res); // move data as needed from a response function\n    };\n\n    const buildteam = (namedSquad) => {\n        const squad = namedSquad.roster_40.queryResults.row\n        // .roster_40.queryResults.row\n        // This is the data structure of the API we are working with\n        \n        const [ pos1, pos2, pos3, pos4, pos5, pos6, pos7, pos8, pos9, ...reserves] = squad;\n        const createSquad = (p1, p2, p3, p4, p5, p6, p7, p8, p9) => \n           ({\n            pitcher: p1,\n            catcher: p2,\n            first: p3,\n            second: p4,\n            short: p5,\n            third: p6,\n            left: p7,\n            center: p8,\n            right: p9\n           })\n        const newSquad = createSquad(pos1, pos2, pos3, pos4, pos5, pos6, pos7, pos8, pos9)\n        console.log({newSquad});\n    }\n\n    console.log(\"theTeam: \", theTeam);        // still a promise!\n}\n\nexport {\n    promises\n}"},"lineCount":null}},"error":null,"hash":"49fc1ab2afaeb2213800fee3238e3f5c","cacheData":{"env":{}}}