ng: 1ch 3ch; */\n    margin: 1ch 0;\n}\n\nh1 span {\n    color: var(--primary-highlight);\n    font-style: italic;\n    font-weight: 200;\n}\n\nh2 {\n    font-weight: 700;\n    font-style: normal;\n    color: var(--white);\n}\n\nheader h4,\nheader h5 {\n    margin: 0;\n    padding: 0;\n    text-transform: capitalize;\n}\n\nheader h5 {\n    font-size: .65rem;\n    font-weight: 300;\n}\n\n/*\n * Interactions\n */\n\n button {\n     cursor: pointer;\n     background-color: var(--primary-highlight);\n     font-size: 1.2rem;\n     border: none;\n     border-radius: 10px;\n     padding: 1ch 2ch;\n     border-bottom-width: 5px;\n     border-bottom-style: solid;\n     border-bottom-color: transparent;\n     border-top-width: 5px;\n     border-top-style: solid;\n     border-top-color: transparent;\n     -webkit-appearance: none;\n        -moz-appearance: none;\n             appearance: none;\n }\n\n button[aria-selected=\"true\"] {\n    background-color: var(--secondary-highlight);\n }\n\n button:not([aria-selected=\"true\"]):hover {\n    border-bottom-color: var(--primary-shade);\n};"},"lineCount":189}},"error":null,"hash":"6ffd323c70a713df9391ff60a8a2edb8","cacheData":{"env":{}}}