omNFT(el);\n            const newElement = document.createElement(`li`);\n            newElement.innerHTML = newNFT;  \n            newElementContainer.appendChild(newElement);\n        })\n\n        newElementContainer.children.length > 0 ? bodyMain.setAttribute(\"data-loading\", \"false\") : bodyMain.setAttribute(\"data-loading\", \"error\");\n    });\n\n    const createNFT = (\n        name,\n        id,\n        collection,\n        img,\n        owner,\n        price,\n        views,\n        favorites,\n        offers,\n        history\n    ) => (\n        {\n            name,\n            id,\n            collection,\n            img,\n            owner,\n            price,\n            views,\n            favorites,\n            offers,\n            history\n        }\n    )\n\n    const createDomNFT = (nft) => {\n        const newElement = createNFT(nft.name, nft.id, nft.collection, nft.img, nft.owner, nft.price, nft.views, nft.favorites, nft.offers, nft.history);\n\n        return `<article data-nft-id=\"${newElement.id}\">\n            <header>\n                <h4>${newElement.name}</h4>\n                <h5>Collection: ${newElement.collection}</h5>\n                <img src=\"${newElement.img}\" alt=\"${newElement.name} from ${newElement.owner} in the ${newElement.collection} collection\" />\n            </header>\n            <ul>\n                <li>Owner: ${newElement.owner}</li>\n                <li>Price: ${newElement.price}</li>\n                <li>Views: ${newElement.views}</li>\n                <li>Favorites: ${newElement.favorites}</li>\n                <li>Offers: ${newElement.offers}</li>\n                <li>History: ${newElement.history}</li>\n            </ul>\n        </article>`\n\n    }\n\n\n    // console.log(newNFT);\n    // console.log(IMAGES[0]);\n    // console.log(newGreeting.nameTo);\n}\n\nexport default NFT;"},"lineCount":null}},"error":null,"hash":"36328840d50068fd614f3ef9186c47e9","cacheData":{"env":{}}}